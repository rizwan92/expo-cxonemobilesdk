{
  "name": "expo-cxonemobilesdk",
  "version": "0.1.16",
  "description": "Mobile SDK lets you integrate CXone Mpower digital chat into your enterprise mobile phone apps.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "expo-module build",
    "clean": "expo-module clean",
    "lint": "expo-module lint",
    "test": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "open:ios": "xed example/ios",
    "open:android": "open -a \"Android Studio\" example/android",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:swift": "swift-format format --in-place --recursive --configuration .swift-format.json ios",
    "format:swift:check": "swift-format format --recursive --configuration .swift-format.json ios"
  },
  "keywords": [
    "react-native",
    "expo",
    "expo-cxonemobilesdk",
    "ExpoCxonemobilesdk"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rizwan92/expo-cxonemobilesdk.git"
  },
  "bugs": {
    "url": "https://github.com/rizwan92/expo-cxonemobilesdk/issues"
  },
  "author": "Rizwan Chauhan <chouhan.rizwan@gmail.com> (rizwan92)",
  "license": "MIT",
  "homepage": "https://github.com/rizwan92/expo-cxonemobilesdk#readme",
  "expo": {
    "plugins": [
      "./plugin/withCXoneSPM"
    ]
  },
  "files": [
    "build",
    "ios",
    "android",
    "plugin",
    "plugins",
    "src",
    "expo-module.config.json"
  ],
  "dependencies": {},
  "peerDependencies": {
    "expo": "*",
    "react": "*",
    "react-native": "*",
    "@expo/config-plugins": "*"
  },
  "exports": {
    "./plugin-spm": "./plugins/addSPMDependenciesToMainTarget.js",
    "./plugin-android-backup": "./plugins/withAndroidFullBackupFix.js"
  },
  "peerDependenciesMeta": {
    "@expo/config-plugins": {
      "optional": false
    }
  },
  "packageManager": "pnpm@9.9.0"
}
